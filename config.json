    "login": {
      "name": "login",
      "path": "/login",
      "method": "POST",
      "headers": { "Content-Type": "application/json" }
    },
    "test": {
      "name": "test",
      "path": "/test",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json",
